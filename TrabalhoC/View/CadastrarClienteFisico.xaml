        <TextBox x:Name="txtNome" HorizontalAlignment="Left" Height="23" Margin="114,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtCpf" HorizontalAlignment="Left" Height="23" Margin="114,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Nome:" HorizontalAlignment="Left" Margin="18,60,0,0" VerticalAlignment="Top" Height="23" Width="57"/>
        <Label Content="CFP:" HorizontalAlignment="Left" Margin="18,122,0,0" VerticalAlignment="Top" Height="23" Width="57"/>
        <Label Content="      Pessoa Fisica" HorizontalAlignment="Left" Margin="71,17,0,0" VerticalAlignment="Top" Height="25" Width="130"/>
        <Label Content="Data de Nascimento:" HorizontalAlignment="Left" Margin="18,188,0,0" VerticalAlignment="Top" Height="23" Width="129"/>
        <DatePicker x:Name="dtpDataDeNascimento" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="152,189,0,0" Height="24" Width="88"/>

    </Grid>
</Window>
